    <Description>Graph-base protocol machines for F#</Description>
    <Description Condition="'$(IsHopac)' == 'true'">$(Description) powered by the Hopac concurrency library</Description>
    <Copyright>Copyright Â© Xyncro Ltd</Copyright>
    <PackageLicenseUrl>https://github.com/xyncro/hephaestus/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://xyncro.tech/hephaestus/</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/xyncro/hephaestus</RepositoryUrl>
    <Tags>protocol;machine;functional;graphs;f#;fsharp</Tags>
    <Tags Condition="'$(IsHopac)' == 'true'">$(Description);hopac</Tags>
  </PropertyGroup>
</Project>
